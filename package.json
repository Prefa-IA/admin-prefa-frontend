{
  "name": "admin-frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.14",
    "axios": "^1.13.2",
    "buffer": "^6.0.3",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.10.0",
    "react-scripts": "5.0.1",
    "react-simple-code-editor": "^0.14.1",
    "react-toastify": "^11.0.5",
    "recharts": "^2.7.0",
    "shpjs": "^3.4.4",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@types/leaflet": "^1.9.5",
    "@types/react": "^18.0.38",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/recharts": "^1.8.24",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-risxss": "^2.0.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-unicorn": "^55.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0",
    "madge": "^7.0.0",
    "prettier": "^3.2.5"
  },
  "scripts": {
    "start": "npx cross-env PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "check:circular": "madge --circular --extensions ts,tsx src/",
    "check:deps": "madge --extensions ts,tsx src/",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "security:check": "npm audit && npm run security:lint",
    "security:lint": "eslint src --ext .ts,.tsx --quiet",
    "security:deps": "npm audit --audit-level=moderate"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
